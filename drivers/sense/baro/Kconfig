menuconfig SPINALI_SENSE_BARO
  bool "Baro"
  help
    This option enables the barometric altimeter driver interface

if SPINALI_SENSE_BARO

config SPINALI_SENSE_BARO_COUNT
  int "Number of barometers"
  default 1
  range 1 4
  help
    Defines number of barometers 1-4

module = SPINALI_SENSE_BARO
module-str = sense_baro
source "subsys/logging/Kconfig.template.log_config"

endif # SPINALI_SENSE_BARO
